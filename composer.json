{
    "name": "websitesql/framework",
    "description": "A simple, easy-to-use, REST API framework.",
    "homepage": "https://www.websitesql.com",
    "minimum-stability": "stable",
    "type": "framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Alan Tiller",
            "email": "alan@alantiller.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "league/climate": "^3.10",
        "vlucas/phpdotenv": "^5.6",
		"league/plates": "^3.6",
        "phpmailer/phpmailer": "^6.9",
		"catfan/medoo": "^2.1",
		"league/route": "^5.0",
		"laminas/laminas-diactoros": "^2.8",
		"laminas/laminas-httphandlerrunner": "^2.1",
		"league/container": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "WebsiteSQL\\Framework\\": "src/"
        },
		"files": [
			"src/Config/Helpers/Env.php"
		]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
